{
    "swagger": "2.0",
    "info": {
      "version": "1.0.0",
      "title": "Conference Room Booking API",
      "description": "API for managing conference room bookings"
    },
    "paths": {
      "/api/auth/register": {
        "post": {
          "summary": "Register a new user",
          "parameters": [
            {
              "name": "body",
              "in": "body",
              "required": true,
              "schema": {
                "type": "object",
                "properties": {
                  "username": {"type": "string"},
                  "password": {"type": "string"},
                  "role": {"type": "string"}
                }
              }
            }
          ],
          "responses": {
            "201": {"description": "User registered successfully"}
          }
        }
      }
    }
  }
  